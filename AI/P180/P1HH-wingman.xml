<?xml version="1.0"?>
<!-- Piaggio P-1HH HammerHead model config file -->
<!-- Paolo Amoroso 2024/11/24 -->

<PropertyList>
	<path>P1HH_wingman.ac</path>
	<offsets>
		<x-m>0</x-m>
	</offsets>

  <!-- Left Propeller --> 
  <animation>
    <name>Lprop</name>
    <object-name>Lpale1</object-name>
    <object-name>Lpale2</object-name>
    <object-name>Lpale3</object-name>
    <object-name>Lpale4</object-name>
    <object-name>Lpale5</object-name>
    <object-name>Lcone</object-name>
    <object-name>propdiscG</object-name>
    <object-name>propblurG</object-name>
  </animation>

  <!-- Right Propeller --> 
  <animation>
    <name>Rprop</name>
    <object-name>Rpale1</object-name>
    <object-name>Rpale2</object-name>
    <object-name>Rpale3</object-name>
    <object-name>Rpale4</object-name>
    <object-name>Rpale5</object-name>
    <object-name>Rcone</object-name>
    <object-name>propdiscD</object-name>
    <object-name>propblurD</object-name>
  </animation>

  <!-- Right propeller -->
  <!-- Selection de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscD</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value>1300</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurD</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[1]/rpm</property>
          <value>400.000</value>
        </greater-than>
        <less-than>
          <property>engines/engine[1]/rpm</property>
          <value>1300.001</value>
        </less-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Rpale1</object-name>
    <object-name>Rpale2</object-name>
    <object-name>Rpale3</object-name>
    <object-name>Rpale4</object-name>
    <object-name>Rpale5</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/rpm</property>
	<value>1300</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>Rprop</object-name>
    <property>engines/engine[1]/rpm</property>
    <factor> 1.5 </factor>
    <center>
      <x-m>  3.000 </x-m>
      <y-m>  2.038 </y-m>
      <z-m> -0.162 </z-m>
    </center>
    <axis>
      <x> 1.0 </x>
      <y> 0.0 </y>
      <z> 0.0 </z>
    </axis>
  </animation>

  <!-- Right blades pitch -->
