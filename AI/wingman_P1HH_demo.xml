<?xml version="1.0"?>

<PropertyList>

	<scenario>

		<name>P.1HH UAV Wingman</name>
		<description>
			Pair - non-tactical
			This scenario formates 1 P.1HH Drone to the
			parent ac at the position designated by
			the x,y,z ofsets.
			Paolo Amoroso
		</description>

		<entry>
			<name>UAV</name>
			<type>wingman</type>
			<model>AI/Aircraft/P180/P1HH-wingman.xml</model>
			<x-offset>-35</x-offset>
			<y-offset>-63</y-offset>
			<z-offset>0</z-offset>
			<formate>true</formate>
			<max-speed-kts>600</max-speed-kts>
			<coefficients>
				<heading>5.0</heading>
				<pitch>4.0</pitch>
				<bank>4.0</bank>
				<speed>2.5</speed>
			</coefficients>
		</entry>

	</scenario>

</PropertyList>
