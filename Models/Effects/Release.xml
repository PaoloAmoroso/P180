    <impact-reports>ai/models/model-impact</impact-reports>
    <submodel-path>Aircraft/P180/Models/Effects/Weapons/Impact/bomb-impact.xml</submodel-path>
    <yaw-offset>0</yaw-offset>
    <pitch-offset>0</pitch-offset>
