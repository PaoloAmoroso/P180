    <wind>false</wind>
    <weight>500</weight>
    <cd>0.295</cd>
    <eda>0.61629</eda>
    <fuse-range>2.2</fuse-range>
    <aero-stabilised>true</aero-stabilised>
    <collision>true</collision>
