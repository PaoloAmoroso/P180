<?xml version="1.0"?>
 
<PropertyList>

	<fx>
    <switch>
      <name>click on</name>
      <mode>once</mode>
      <path>Aircraft/P180/Sounds/click.wav</path>
      <condition>
        <and>
          <property>/sim/sound/switch</property>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume>
        <factor>1.80</factor>
      </volume>
    </switch>
    
    <switch>
      <name>click off</name>
      <mode>once</mode>
      <path>Aircraft/P180/Sounds/click.wav</path>
      <condition>
        <and>
          <not>
             <property>/sim/sound/switch</property>
          </not>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume>
        <factor>1.80</factor>
      </volume>
    </switch>
    
        <thunder1>
            <name>thunder1</name>
            <mode>once</mode>
            <path>Sounds/thunder1.wav</path>
            <property>/sim/model/p180/sound/click-thunder1</property>
            <volume>4</volume>
        </thunder1>
        <thunder2>
            <name>thunder2</name>
            <mode>once</mode>
            <path>Sounds/thunder2.wav</path>
            <property>/sim/model/p180/sound/click-thunder2</property>
            <volume>4</volume>
        </thunder2>
        <thunder3>
            <name>thunder3</name>
            <mode>once</mode>
            <path>Sounds/thunder3.wav</path>
            <property>/sim/model/p180/sound/click-thunder3</property>
            <volume>4</volume>
        </thunder3>

        <rain>
            <name>rain</name>
            <mode>looped</mode>
            <path>Sounds/rain.wav</path>
            <condition>
                <greater-than>
                    <property>/environment/rain-norm</property>
                    <value>0</value>
                </greater-than>
            </condition>
            <volume>
                <factor>0.50</factor>
            </volume>
        </rain>

        <rain-windshield>
            <name>rain-windshield</name>
            <mode>looped</mode>
            <path>Sounds/rain-windshield.wav</path>
            <condition>
                <and>
                    <greater-than>
                        <property>/environment/rain-norm</property>
                        <value>0</value>
                    </greater-than>
                    <property>/sim/current-view/internal</property>
                </and>
            </condition>
            <position>
                <x>-3.0</x>
                <y>0.0</y>
                <z>0.5</z>
            </position>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.014</factor>
                <min>0.05</min>
            </volume>
            <volume>
                <offset>0.07</offset>
            </volume>
        </rain-windshield>
<!-- Engine 1 -->
		<crank>
   			<name>engstart0</name>
   			<mode>once</mode>
   			<path>Sounds/turbine_start.wav</path>
   			<condition>
     			<greater-than>
       				<property>/controls/engines/engine[0]/condition</property>
       				<value>0</value>
     			</greater-than>
   			</condition>
   			<position>
     			<x>7</x>
     			<y>-1.5</y>
     			<z>0</z>
   			</position>
   			<reference-dist>2.0</reference-dist>
   			<max-dist>100.0</max-dist>
  		</crank>

		<crank>
   			<name>engstart1</name>
   			<mode>once</mode>
   			<path>Sounds/turbine_start.wav</path>
   			<condition>
     			<greater-than>
       				<property>/controls/engines/engine[1]/condition</property>
       				<value>0</value>
     			</greater-than>
   			</condition>
   			<position>
     			<x>7</x>
     			<y>1.5</y>
     			<z>3</z>
   			</position>
   			<reference-dist>2.0</reference-dist>
   			<max-dist>100.0</max-dist>
  		</crank>

		<prop>
    		<name>prop1in</name>
    		<mode>looped</mode>
    		<path>Sounds/turboprop1.wav</path>
    		<condition>
                <and>
    			<greater-than>
        			<property>/engines/engine[0]/rpm</property>
        			<value>5</value>
      			</greater-than>
                    <property>/sim/current-view/internal</property>
                </and>
      		</condition>
    		<volume>
      			<property>/engines/engine[0]/rpm</property>
      			<factor>0.00020</factor>
   	 	    </volume>
    		<pitch>
      			<property>/engines/engine[0]/rpm</property>
      			<offset>0.05</offset>
      			<factor>0.0055</factor>
      			<min>0.03</min>
      			<max>1.05</max>
    		</pitch>
    		<position>
      			<x>8</x>
      			<y>-1.7</y>
      			<z>0</z>
    		</position>
  		</prop>

		<prop>
    		<name>prop2in</name>
    		<mode>looped</mode>
    		<path>Sounds/turboprop1.wav</path>
    		<condition>
                <and>
    			<greater-than>
        			<property>/engines/engine[1]/rpm</property>
        			<value>5</value>
      			</greater-than>
                    <property>/sim/current-view/internal</property>
                </and>
      		</condition>
    		<volume>
      			<property>/engines/engine[1]/rpm</property>
      			<factor>0.00020</factor>
   	 	    </volume>
    		<pitch>
      			<property>/engines/engine[1]/rpm</property>
      			<offset>0.05</offset>
      			<factor>0.0055</factor>
      			<min>0.03</min>
      			<max>1.05</max>
    		</pitch>
    		<position>
      			<x>8</x>
      			<y>1.7</y>
      			<z>0</z>
    		</position>
  		</prop>
        
            <prop>
    		<name>prop1</name>
    		<mode>looped</mode>
    		<path>Sounds/turboprop1.wav</path>
    		<condition>
                <and>
    			<greater-than>
        			<property>/engines/engine[0]/rpm</property>
        			<value>1</value>
      			</greater-than>
                <not>
                    <property>/sim/current-view/internal</property>
                </not>
                </and>
      		</condition>
    		<volume>
      			<property>/engines/engine[0]/rpm</property>
      			<factor>0.00040</factor>
   	 	    </volume>
    		<pitch>
      			<property>/engines/engine[0]/rpm</property>
      			<offset>0.1</offset>
      			<factor>0.0085</factor>
      			<min>0.05</min>
      			<max>1.1</max>
    		</pitch>
    		<position>
      			<x>7</x>
      			<y>-1.5</y>
      			<z>0</z>
    		</position>
  		</prop>

		<prop>
    		<name>prop2</name>
    		<mode>looped</mode>
    		<path>Sounds/turboprop1.wav</path>
    		<condition>
                <and>
    			<greater-than>
        			<property>/engines/engine[1]/rpm</property>
        			<value>1</value>
      			</greater-than>
                <not>
                    <property>/sim/current-view/internal</property>
                </not>
                </and>
      		</condition>
    		<volume>
      			<property>/engines/engine[1]/rpm</property>
      			<factor>0.00040</factor>
   	 	    </volume>
    		<pitch>
      			<property>/engines/engine[1]/rpm</property>
      			<offset>0.1</offset>
      			<factor>0.0085</factor>
      			<min>0.05</min>
      			<max>1.1</max>
    		</pitch>
    		<position>
      			<x>7</x>
      			<y>1.5</y>
      			<z>0</z>
    		</position>
  		</prop>
  
		<turbine0>
    		<name>turbine0</name>
    		<mode>looped</mode>
    		<path>Aircraft/P180/Sounds/engine_running_1.wav</path>
    		<condition>
        			<property>/engines/engine/running</property>
    		</condition>
    		<volume>
      			<property>/engines/engine/rpm</property>
      			<factor>0.0047</factor>
                <min>0.6</min>
                <max>4.0</max>
      		</volume>
            <pitch>
                <property>/engines/engine/rpm</property>
                <factor>0.0018</factor>
                <min>0.05</min>
                <max>1.1</max>
                <offset>0.25</offset>
            </pitch>
    		<position>
      			<x>6</x>
      			<y>-1.5</y>
      			<z>0</z>
    		</position>
		</turbine0>
 
		<turbine1>
    		<name>turbine1</name>
    		<mode>looped</mode>
    		<path>Aircraft/P180/Sounds/engine_running_1.wav</path>
    		<condition>
        			<property>/engines/engine[1]/running</property>
    		</condition>
    		<volume>
      			<property>/engines/engine[1]/rpm</property>
      			<factor>0.0047</factor>
                <min>0.6</min>
                <max>4.0</max>
      		</volume>
            <pitch>
                <property>/engines/engine[1]/rpm</property>
                <factor>0.0018</factor>
                <min>0.05</min>
                <max>1.1</max>
                <offset>0.25</offset>
            </pitch>
    		<position>
      			<x>6</x>
      			<y>1.5</y>
      			<z>0</z>
    		</position>
		</turbine1>
  
		<shutdown>
    		<name>engine0 off</name>
    		<mode>once</mode>
    		<path>Sounds/turbine_shutdown.wav</path>
    		<condition>
            <and>
      			<equals>
        			<property>/controls/engines/engine[0]/condition</property>
        			<value>0</value>
      			</equals>
                <equals>
        			<property>/controls/init_starting</property>
        			<value>0</value>
      			</equals>
            </and>
    		</condition>
    		<position>
      			<x>7</x>
      			<y>-1.5</y>
      			<z>0</z>
    		</position>
   			<reference-dist>2.0</reference-dist>
   			<max-dist>100.0</max-dist>
  		</shutdown>

		<shutdown>
    		<name>engine1 off</name>
    		<mode>once</mode>
    		<path>Sounds/turbine_shutdown.wav</path>
    		<condition>
            <and>
      			<equals>
        			<property>/controls/engines/engine[1]/condition</property>
        			<value>0</value>
      			</equals>
                <equals>
        			<property>/controls/init_starting</property>
        			<value>0</value>
      			</equals>
            </and>
    		</condition>
    		<position>
      			<x>7</x>
      			<y>1.5</y>
      			<z>0</z>
    		</position>
   			<reference-dist>2.0</reference-dist>
   			<max-dist>100.0</max-dist>
  		</shutdown>

		<rumble>
   			<name>rumble</name>
   			<mode>looped</mode>
   			<path>Sounds/rumble.wav</path>
   			<condition>
    			<or>
     				<property>/gear/gear[0]/wow</property>
     				<property>/gear/gear[1]/wow</property>
     				<property>/gear/gear[2]/wow</property>
    			</or>
   			</condition>
   			<volume>
     			<property>/velocities/airspeed-kt</property>
     			<type>log</type>
     			<factor>0.7</factor>
     			<max>3.5</max>
   			</volume>
   			<reference-dist>1.0</reference-dist>
   			<max-dist>100.0</max-dist>
  		</rumble>

		<gear>
      		<name>gear</name>
      		<mode>in-transit</mode>
      		<path>Sounds/gear.wav</path>
      		<property>/gear/gear[0]/position-norm</property>
      		<pitch>
       			<internal>dt_play</internal>
        		<offset>0.9</offset>
        		<factor>-0.001</factor>
      		</pitch>
   			<reference-dist>1.0</reference-dist>
   			<max-dist>100.0</max-dist>
    	</gear>

		<gear>
      		<name>gear-lock</name>
      		<path>Sounds/gear-lck.wav</path>
      		<condition>
        		<equals>
          			<property>/gear/gear[0]/position-norm</property>
          			<value>0</value>
        		</equals>
      		</condition>
   			<reference-dist>1.0</reference-dist>
   			<max-dist>100.0</max-dist>
    	</gear>

		<squeal>
   			<name>squeal</name>
   			<path>Sounds/squeal.wav</path>
   			<condition>
    			<or>
     				<property>/gear/gear[0]/wow</property>
     				<property>/gear/gear[1]/wow</property>
     				<property>/gear/gear[2]/wow</property>
    			</or>
   			</condition>
   			<volume>
    			<property>/velocities/airspeed-kt</property>
    			<factor>0.05</factor>
   			</volume>
			<pitch>
    			<property>/velocities/airspeed-kt</property>
    			<factor>0.0030</factor>
    			<offset>1.0</offset>
   			</pitch>
  		</squeal>
 
		<flap>
   			<name>flaps</name>
   			<mode>in-transit</mode>
   			<path>Sounds/flaps.wav</path>
   			<property>/surface-positions/flap-pos-norm</property>
   			<reference-dist>1.0</reference-dist>
   			<max-dist>100.0</max-dist>
  		</flap>

		<wind>
   			<name>wind</name>
   			<mode>looped</mode>
   			<path>Sounds/wind1.wav</path>
   			<property>/velocities/airspeed-kt</property>
   			<volume>
    			<property>/position/altitude-ft</property>
    			<factor>-0.000014</factor>
    			<offset>0.5</offset>
    			<min>0.02</min>
    			<max>0.6</max>
   			</volume>
   			<volume>
     			<property>/velocities/airspeed-kt</property>
     			<factor>0.0060</factor>
     			<min>0.01</min>
     			<max>5.0</max>
   			</volume>
   			<pitch>
    			<property>/velocities/airspeed-kt</property>
    			<factor>0.001</factor>
    			<offset>0.1</offset>
   			</pitch>
		</wind>
 
	</fx>
 
</PropertyList>
